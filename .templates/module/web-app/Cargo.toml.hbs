[profile.dev]
opt-level = "z"

[profile.release]
opt-level = "z"

[workspace]
members = ["zomes/coordinator/*", "zomes/integrity/*"]
resolver = "2"

[workspace.dependencies]
holochain = { version = "{{holochain_version}}", default-features = false, features = [
  "test_utils",
] }
hdk = "0.2"
hdi = "0.3"
serde = "1"

